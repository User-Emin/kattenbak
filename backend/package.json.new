{
  "name": "kattenbak-backend",
  "version": "2.0.0",
  "description": "Enterprise e-commerce backend with Advanced RAG",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --noEmitOnError false && tsc-alias || true",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "rag:ingest": "tsx src/services/rag/document-ingestion.service.ts ingest",
    "rag:clear": "tsx src/services/rag/document-ingestion.service.ts clear",
    "rag:count": "tsx src/services/rag/document-ingestion.service.ts count",
    "rag:eval": "tsx src/services/rag/evaluation.service.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:rag": "jest src/__tests__/rag-*.test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@langchain/community": "^0.3.20",
    "@langchain/core": "^0.3.30",
    "langchain": "^0.3.10",
    "langsmith": "^0.2.20",
    "@mollie/api-client": "^4.3.3",
    "@prisma/client": "^6.1.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.0.4",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
